<%= form_for @user do |f| %>
<%= f.label value="NAME" %>
<%= f.text_field :name %></br>
<%= f.label value="EMAIL" %>
<%= f.text_field :email %></br>
<%= f.label value="PASSWORD" %>
<%= f.password_field :password %></br>
<%= f.label value="CONFIRM PASSWORD" %>
<%= f.password_field :password_confirmation %></br>
<%= f.submit "Create Account" %>
<% end %>
